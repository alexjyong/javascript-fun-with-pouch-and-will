// Your code here.
counter = 1;
while (counter < 101) {
 
  if (counter % 3 == 0 && counter % 5 != 0 ) {
      console.log("Fizz");
      }

  else if (counter % 5 == 0 && counter % 3 !=0) {
    console.log("Buzz");
  }
  
  else if (counter % 5 == 0 && counter % 3 ==0) {
    console.log("FizzBuzz");
  }
  else {
    console.log(counter);
  }
  
  counter = counter + 1;
  
}
